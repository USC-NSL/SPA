#!/bin/sh

CFLAGS='-O0 -g -I/usr/local/bin/spa/examples/exosip/libosip2-3.6.0/include' \
CXXFLAGS='-O0 -g -I/usr/local/bin/spa/examples/exosip/libosip2-3.6.0/include' \
AR='echo' RANLIB='echo' ./configure \
--disable-shared \
--enable-static \
--disable-tools \
--enable-debug \
--disable-trace \
--disable-mpatrol \
--disable-gprof \
--disable-mt \
--disable-openssl \
--disable-srvrec
