#!/bin/sh

CC='clang' CFLAGS="-DENABLE_SPA -DENABLE_KLEE -Wall -Weverything -Wno-return-type -I`pwd`/../../include -emit-llvm" \
CXX='clang++' CXXFLAGS="-DENABLE_SPA -DENABLE_KLEE -Wall -Weverything -Wno-return-type -I`pwd`/../../include -emit-llvm" \
LD='llvm-link' \
CPP='clang -E' \
make libowfat.a
