#managed-mode-passphrase-page {
  min-width: 350px;
  padding: 5px 15px;
}

#managed-user-logo {
  background-image: url('options/managed_user.png');
  height: 68px;
  width: 65px;
}

#entry {
  margin: 15px 0;
}

#passphrase-entry {
  width: 100%;
}

#buttons {
  float: right;
}

html[dir=rtl] #buttons {
  float: left;
}

#incorrect-passphrase-warning {
  color: red;
  font-weight: bold;
}
