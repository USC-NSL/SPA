{
  "name": "Cloud File Picker",
  "version": "1.0.0.1",
  "icons" : {
    "128" : "128.png"
  },
  "app" : {
    "urls" : ["http://www.cloudfilepicker.com/", "http://www.cfp.com/intents"],
    "launch" : {
      "web_url" : "http://www.cloudfilepicker.com/"
    }
  },
  "intents": {
    "http://webintents.org/edit" : {
      "type" : ["image/*"],
      "href" : "http://www.cfp.com/intents/edit.html",
