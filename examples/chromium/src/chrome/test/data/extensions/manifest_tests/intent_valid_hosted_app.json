    },
    "http://webintents.org/pick" : {
      "type" : ["image/*"],
      "href" : "http://www.cloudfilepicker.com/",
      "title" : "Pick file from CloudFilePicker.com"
    },
    "http://webintents.org/share" : {
      "type" : ["image/*"],
      "href" : "http://www.cloudfilepicker.com/intents/share.html",
      "title" : "Share file from CloudFilePicker.com"
    }
  }
}
